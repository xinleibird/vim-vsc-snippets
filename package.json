{
  "name": "vim-vsc-snippets",
  "engines": {
    "vscode": "^1.11.0"
  },
  "contributes": {
    "snippets": [
      {
        "language": "groovy",
        "path": "./snippets/groovy.json"
      },
      {
        "language": "html",
        "path": "./snippets/html.json"
      },
      {
        "language": [
          "javascript",
          "javascriptreact",
          "vue",
          "svelte"
        ],
        "path": "./snippets/javascript.json"
      },
      {
        "language": [
          "markdown",
          "rmd"
        ],
        "path": "./snippets/markdown.json"
      },
      {
        "language": "php",
        "path": "./snippets/php.json"
      },
      {
        "language": "swift",
        "path": "./snippets/swift.json"
      },
      {
        "language": [
          "typescript",
          "typescriptreact"
        ],
        "path": "./snippets/typescript.json"
      },
      {
        "language": [
          "vb",
          "vbnet",
          "visualbasic"
        ],
        "path": "./snippets/vb.json"
      }
    ]
  }
}
